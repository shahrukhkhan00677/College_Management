<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
<br />
<br />
@{
    if (TempData["Datadeleted"] != null)
    {
        <div class="alert alert-danger col-4 offset-4 alert-dismissible " id="notification">
            @TempData["Datadeleted"]
        </div>
    }

    if (TempData["fbresult"] != null)
    {
        <div class="alert alert-success col-4 offset-4 alert-dismissible " id="notification">
            @TempData["fbresult"]
        </div>
    }

    if (TempData["InsertContact"] != null)
    {
        <div class="alert alert-success col-4 offset-4 alert-dismissible " id="notification">
            @TempData["InsertContact"]
        </div>
    }
    
}


<script>
    $(document).ready(function () {

        if ($("#notification").length) {
            setTimeout(() => {
                $("#notification").fadeOut('1000');
            }, 1000)

        };

    });
</script>

